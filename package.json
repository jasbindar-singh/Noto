{
  "name": "noto",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^7.14.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
