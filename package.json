    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@iconify/icons-bx": "^1.0.3",
    "@iconify/react": "^1.1.3",
    "autoprefixer": "^9.7.6",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.3.5"
